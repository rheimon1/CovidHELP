{
  "expo": {
    "name": "CovidHELP",
    "description": "Aplicação que mapeia pessoas que estão precisando de ajuda em meio ao momento de pandemia",
    "slug": "covidhelp",
    "sdkVersion": "38.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "updates": { 
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fsiproject.covidhelp"
    },
    "android": {
      "package": "com.fsiproject.covidhelp"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#FFF"
    }
  }
}
